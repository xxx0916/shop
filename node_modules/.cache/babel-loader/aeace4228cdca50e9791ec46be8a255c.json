{"ast":null,"code":"import SearchSelector from './SearchSelector/SearchSelector';\nimport { mapGetters } from 'vuex';\nexport default {\n  name: 'Search',\n\n  data() {\n    return {\n      // 带个服务器的参数\n      searchParams: {\n        // 一级分类的id\n        category1Id: \"\",\n        // 二级分类id\n        category2Id: \"\",\n        // 三级分类id\n        category3Id: \"\",\n        // 分类名字\n        categoryName: \"\",\n        // 关键字\n        keyword: \"\",\n        // 排序:初始状态应该是综合|降序\n        order: \"1:desc\",\n        // 分页器：代表的是当前是第几页\n        pageNo: 1,\n        // 代表每一页展示数据的个数\n        pageSize: 3,\n        // 平台售卖属性操作带的参数\n        props: [],\n        // 品牌\n        trademark: \"\"\n      }\n    };\n  },\n\n  components: {\n    SearchSelector\n  },\n\n  // 在组件挂载完毕之前执行一次【先与mounted之前】\n  beforeMount() {\n    // 复杂的写法\n    // this.searchParams.category1Id=this.$route.query.category1Id;\n    // this.searchParams.category2Id=this.$route.query.category2Id;\n    // this.searchParams.category3Id=this.$route.query.category3Id;\n    // Object.assign:ES6新增的语法，合并对象\n    Object.assign(this.searchParams, this.$route.query, this.$route.params);\n  },\n\n  // 组件挂载完毕执行一次（只执行一次）\n  mounted() {\n    // 在发送请求之前带给服务器参数【searchParams参数发生变化有数值带个服务器】\n    this.getData();\n  },\n\n  computed: { ...mapGetters(['goodsList']),\n\n    isOne() {\n      return this.searchParams.order.indexOf('1') != -1;\n    },\n\n    isTwo() {\n      return this.searchParams.order.indexOf('2') != -1;\n    },\n\n    isAsc() {\n      return this.searchParams.order.indexOf('asc') != -1;\n    },\n\n    isDesc() {\n      return this.searchParams.order.indexOf('desc') != -1;\n    }\n\n  },\n  methods: {\n    // 向服务器发请求获取search模块数据（根据参数不同返回不同的数据进行展示—）\n    // 把这次请求封装为一个函数：当你需要调用的时候即可调用\n    getData() {\n      // 派发action,\n      this.$store.dispatch('getSearchList', this.searchParams);\n    },\n\n    // 删除分类的名字（面包屑）\n    removeCategoryName() {\n      // 把带个服务器的参数清空，还需要向服务器发请求 \n      // 从性能角度出发，属性值为undefined不会带给服务器\n      // 带给服务器参数说明可有可无的：如果属性值为空的字符串还是会把相应的字段带给服务器\n      // 但是把相应字段变为undefined，当前这个字段不会带服务器\n      this.searchParams.categoryName = undefined;\n      this.searchParams.category1Id = undefined;\n      this.searchParams.category2Id = undefined;\n      this.searchParams.category3Id = undefined;\n      this.getData(); // 地址栏也需要修改：进行路由跳转(现在的路由跳转只是跳转到自己这里)\n      // 严谨：本意是删除query,如果路径当中出现params不应该删除，路由跳转的时候应该还需要带着\n\n      if (this.$route.params) {\n        this.$router.push({\n          name: \"search\",\n          params: this.$route.params\n        });\n      }\n    },\n\n    // 删除关键字\n    removeKeyword() {\n      // 给服务器带的参数searchParams的keyword置空\n      this.searchParams.keyword = undefined; // 再次发送请求\n\n      this.getData(); // 通知兄弟组件Header清除关键字\n\n      this.$bus.$emit('clear'); // 进行路由的跳转\n\n      if (this.$route.query) {\n        this.$router.push({\n          name: 'search',\n          query: this.$route.query\n        });\n      }\n    },\n\n    // 自定义事件的回调\n    trademarkInfo(trademark) {\n      // 1:整理品牌字段的参数 “ID:品牌名称”\n      this.searchParams.trademark = `${trademark.tmId}:${trademark.tmName}`; // 再次发请求获取search模块列表数据进行展示\n\n      this.getData();\n    },\n\n    // 删除品牌的面包屑\n    removeTradeMark() {\n      // 将品牌信息清空\n      this.searchParams.trademark = undefined; // 再次发请求\n\n      this.getData();\n    },\n\n    // 收集平台属性地方回调函数（自定义事件）\n    attrInfo(attr, attrValue) {\n      // [\"属性ID:属性值：属性名\"]\n      // 参数格式整理好\n      let props = `${attr.attrId}:${attrValue}:${attr.attrName}`; // 数组去重\n      // if语句里面只要一条，可以省略大花括号\n\n      if (this.searchParams.props.indexOf(props) == -1) {\n        this.searchParams.props.push(props);\n      } // 再次发送请求\n\n\n      this.getData();\n    },\n\n    // 删除售卖的属性\n    removeAttr(index) {\n      // 再次整理参数\n      this.searchParams.props.splice(index, 1); // 再次发请求\n\n      this.getData();\n    },\n\n    // 排序的操作\n    changeOrder(flag) {\n      // flage形参：标记，1是综合 2是价格\n      console.log(flag);\n    }\n\n  },\n  // 数据监听：监听组件实例身上的属性的属性值变化\n  watch: {\n    // 监听路由的信息是否发生变化，如果发生变化，再次发起请求\n    $route(newValue, oldValue) {\n      // 再次发请求之前整理带给服务器的参数\n      Object.assign(this.searchParams, this.$route.query, this.$route.params); // 再次发送请求\n\n      this.getData(); // 每一次请求，都一个吧相应的1,2,3级分类的id置空，让他接收下一次的\n\n      this.searchParams.category1Id = undefined;\n      this.searchParams.category2Id = undefined;\n      this.searchParams.category3Id = undefined;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkHA;AACA;AACA;EACAA,cADA;;EAEAC;IACA;MACA;MACAC;QACA;QACAC,eAFA;QAGA;QACAC,eAJA;QAKA;QACAC,eANA;QAOA;QACAC,gBARA;QASA;QACAC,WAVA;QAWA;QACAC,eAZA;QAaA;QACAC,SAdA;QAeA;QACAC,WAhBA;QAiBA;QACAC,SAlBA;QAmBA;QACAC;MApBA;IAFA;EAyBA,CA5BA;;EA6BAC;IACAC;EADA,CA7BA;;EAgCA;EACAC;IACA;IACA;IACA;IACA;IACA;IACAC;EAEA,CAzCA;;EA0CA;EACAC;IACA;IACA;EACA,CA9CA;;EA+CAC,YACA,4BADA;;IAEAC;MACA;IACA,CAJA;;IAKAC;MACA;IACA,CAPA;;IAQAC;MACA;IACA,CAVA;;IAWAC;MACA;IACA;;EAbA,CA/CA;EA8DAC;IACA;IACA;IACAC;MACA;MACA;IACA,CANA;;IAOA;IACAC;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA,eATA,CAUA;MACA;;MACA;QACA;UAAAzB;UAAA0B;QAAA;MACA;IACA,CAvBA;;IAwBA;IACAC;MACA;MACA,sCAFA,CAGA;;MACA,eAJA,CAKA;;MACA,yBANA,CAOA;;MACA;QACA;UAAA3B;UAAA4B;QAAA;MACA;IACA,CApCA;;IAqCA;IACAC;MACA;MACA,sEAFA,CAGA;;MACA;IACA,CA3CA;;IA4CA;IACAC;MACA;MACA,wCAFA,CAGA;;MACA;IACA,CAlDA;;IAmDA;IACAC;MACA;MACA;MACA,2DAHA,CAIA;MACA;;MACA;QACA;MACA,CARA,CASA;;;MACA;IACA,CA/DA;;IAgEA;IACAC;MACA;MACA,yCAFA,CAGA;;MACA;IACA,CAtEA;;IAuEA;IACAC;MACA;MACAC;IAEA;;EA5EA,CA9DA;EA4IA;EACAC;IACA;IACAC;MACA;MACApB,wEAFA,CAGA;;MACA,eAJA,CAKA;;MACA;MACA;MACA;IACA;;EAXA;AA7IA","names":["name","data","searchParams","category1Id","category2Id","category3Id","categoryName","keyword","order","pageNo","pageSize","props","trademark","components","SearchSelector","beforeMount","Object","mounted","computed","isOne","isTwo","isAsc","isDesc","methods","getData","removeCategoryName","params","removeKeyword","query","trademarkInfo","removeTradeMark","attrInfo","removeAttr","changeOrder","console","watch","$route"],"sourceRoot":"src/pages/Search","sources":["index.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 商品三级分类列表 -->\r\n    <TypeNav />\r\n    <div class=\"main\">\r\n      <div class=\"py-container\">\r\n        <!--bread 面包屑-->\r\n        <div class=\"bread\">\r\n          <ul class=\"fl sui-breadcrumb\">\r\n            <li>\r\n              <a href=\"#\">全部结果</a>\r\n            </li>\r\n          </ul>\r\n          <ul class=\"fl sui-tag\">\r\n            <!-- 分类的面包屑 -->\r\n            <li class=\"with-x\" v-if=\"searchParams.categoryName\">{{ searchParams.categoryName }}<i\r\n                @click=\"removeCategoryName\">×</i></li>\r\n            <!-- 关键字的面包屑 -->\r\n            <li class=\"with-x\" v-if=\"searchParams.keyword\">{{ searchParams.keyword }}<i @click=\"removeKeyword\">×</i></li>\r\n            <!-- 品牌的面包屑 -->\r\n            <li class=\"with-x\" v-if=\"searchParams.trademark\">{{ searchParams.trademark.split(\":\")[1] }}<i\r\n                @click=\"removeTradeMark\">×</i></li>\r\n            <!-- 平台的售卖的属性值展示 -->\r\n            <li class=\"with-x\" v-for=\"(attrValue, index) in searchParams.props\" :key=\"index\">\r\n              {{ attrValue.split(\":\")[1] }}<i @click=\"removeAttr(index)\">×</i></li>\r\n          </ul>\r\n        </div>\r\n\r\n        <!--selector-->\r\n        <SearchSelector @trademarkInfo=\"trademarkInfo\" @attrInfo=\"attrInfo\" />\r\n\r\n        <!--details-->\r\n        <div class=\"details clearfix\">\r\n          <div class=\"sui-navbar\">\r\n            <div class=\"navbar-inner filter\">\r\n              <!-- 排序的结构 -->\r\n              <ul class=\"sui-nav\">\r\n                <li :class=\"{active:isOne}\" @click=\"changeOrder('1')\">\r\n                  <a>综合<i v-show=\"isOne\" :class=\"{'el-icon-sort-down':isDesc,'el-icon-sort-up':isAsc}\"></i></a>\r\n                </li>\r\n                <li :class=\"{active:isTwo}\" @click=\"changeOrder('2')\">\r\n                  <a >价格<i v-show=\"isTwo\" :class=\"{'el-icon-sort-down':isDesc,'el-icon-sort-up':isAsc}\"></i></a>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n          </div>\r\n          <!-- 销售产品列 -->\r\n          <div class=\"goods-list\">\r\n            <ul class=\"yui3-g\">\r\n              <li class=\"yui3-u-1-5\" v-for=\"(good, index) in goodsList\" :key=\"good.id\">\r\n                <div class=\"list-wrap\">\r\n                  <div class=\"p-img\">\r\n                    <a href=\"item.html\" target=\"_blank\">\r\n                      <img :src=\"good.defaultImg\" />\r\n                    </a>\r\n                  </div>\r\n                  <div class=\"price\">\r\n                    <strong>\r\n                      <em>¥</em>&nbsp;\r\n                      <i>{{ good.price }}</i>\r\n                    </strong>\r\n                  </div>\r\n                  <div class=\"attr\">\r\n                    <a target=\"_blank\" href=\"item.html\" title=\"促销信息，下单即赠送三个月CIBN视频会员卡！【小米电视新品4A 58 火爆预约中】\">{{ good.title\r\n                    }}</a>\r\n                  </div>\r\n                  <div class=\"commit\">\r\n                    <i class=\"command\">已有<span>2000</span>人评价</i>\r\n                  </div>\r\n                  <div class=\"operate\">\r\n                    <a href=\"success-cart.html\" target=\"_blank\" class=\"sui-btn btn-bordered btn-danger\">加入购物车</a>\r\n                    <a href=\"javascript:void(0);\" class=\"sui-btn btn-bordered\">收藏</a>\r\n                  </div>\r\n                </div>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n          <!-- 分页器 -->\r\n          <div class=\"fr page\">\r\n            <div class=\"sui-pagination clearfix\">\r\n              <ul>\r\n                <li class=\"prev disabled\">\r\n                  <a href=\"#\">«上一页</a>\r\n                </li>\r\n                <li class=\"active\">\r\n                  <a href=\"#\">1</a>\r\n                </li>\r\n                <li>\r\n                  <a href=\"#\">2</a>\r\n                </li>\r\n                <li>\r\n                  <a href=\"#\">3</a>\r\n                </li>\r\n                <li>\r\n                  <a href=\"#\">4</a>\r\n                </li>\r\n                <li>\r\n                  <a href=\"#\">5</a>\r\n                </li>\r\n                <li class=\"dotted\"><span>...</span></li>\r\n                <li class=\"next\">\r\n                  <a href=\"#\">下一页»</a>\r\n                </li>\r\n              </ul>\r\n              <div><span>共10页&nbsp;</span></div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SearchSelector from './SearchSelector/SearchSelector'\r\nimport { mapGetters } from 'vuex'\r\nexport default {\r\n  name: 'Search',\r\n  data() {\r\n    return {\r\n      // 带个服务器的参数\r\n      searchParams: {\r\n        // 一级分类的id\r\n        category1Id: \"\",\r\n        // 二级分类id\r\n        category2Id: \"\",\r\n        // 三级分类id\r\n        category3Id: \"\",\r\n        // 分类名字\r\n        categoryName: \"\",\r\n        // 关键字\r\n        keyword: \"\",\r\n        // 排序:初始状态应该是综合|降序\r\n        order: \"1:desc\",\r\n        // 分页器：代表的是当前是第几页\r\n        pageNo: 1,\r\n        // 代表每一页展示数据的个数\r\n        pageSize: 3,\r\n        // 平台售卖属性操作带的参数\r\n        props: [],\r\n        // 品牌\r\n        trademark: \"\"\r\n      }\r\n    }\r\n  },\r\n  components: {\r\n    SearchSelector\r\n  },\r\n  // 在组件挂载完毕之前执行一次【先与mounted之前】\r\n  beforeMount() {\r\n    // 复杂的写法\r\n    // this.searchParams.category1Id=this.$route.query.category1Id;\r\n    // this.searchParams.category2Id=this.$route.query.category2Id;\r\n    // this.searchParams.category3Id=this.$route.query.category3Id;\r\n    // Object.assign:ES6新增的语法，合并对象\r\n    Object.assign(this.searchParams, this.$route.query, this.$route.params)\r\n\r\n  },\r\n  // 组件挂载完毕执行一次（只执行一次）\r\n  mounted() {\r\n    // 在发送请求之前带给服务器参数【searchParams参数发生变化有数值带个服务器】\r\n    this.getData();\r\n  },\r\n  computed: {\r\n    ...mapGetters(['goodsList']),\r\n    isOne(){\r\n      return this.searchParams.order.indexOf('1')!=-1\r\n    },\r\n    isTwo(){\r\n      return this.searchParams.order.indexOf('2')!=-1\r\n    },\r\n    isAsc(){\r\n      return this.searchParams.order.indexOf('asc')!=-1\r\n    },\r\n    isDesc(){\r\n      return this.searchParams.order.indexOf('desc')!=-1\r\n    }\r\n  },\r\n  methods: {\r\n    // 向服务器发请求获取search模块数据（根据参数不同返回不同的数据进行展示—）\r\n    // 把这次请求封装为一个函数：当你需要调用的时候即可调用\r\n    getData() {\r\n      // 派发action,\r\n      this.$store.dispatch('getSearchList', this.searchParams)\r\n    },\r\n    // 删除分类的名字（面包屑）\r\n    removeCategoryName() {\r\n      // 把带个服务器的参数清空，还需要向服务器发请求 \r\n      // 从性能角度出发，属性值为undefined不会带给服务器\r\n      // 带给服务器参数说明可有可无的：如果属性值为空的字符串还是会把相应的字段带给服务器\r\n      // 但是把相应字段变为undefined，当前这个字段不会带服务器\r\n      this.searchParams.categoryName = undefined;\r\n      this.searchParams.category1Id = undefined;\r\n      this.searchParams.category2Id = undefined;\r\n      this.searchParams.category3Id = undefined;\r\n      this.getData();\r\n      // 地址栏也需要修改：进行路由跳转(现在的路由跳转只是跳转到自己这里)\r\n      // 严谨：本意是删除query,如果路径当中出现params不应该删除，路由跳转的时候应该还需要带着\r\n      if (this.$route.params) {\r\n        this.$router.push({ name: \"search\", params: this.$route.params })\r\n      }\r\n    },\r\n    // 删除关键字\r\n    removeKeyword() {\r\n      // 给服务器带的参数searchParams的keyword置空\r\n      this.searchParams.keyword = undefined;\r\n      // 再次发送请求\r\n      this.getData();\r\n      // 通知兄弟组件Header清除关键字\r\n      this.$bus.$emit('clear')\r\n      // 进行路由的跳转\r\n      if (this.$route.query) {\r\n        this.$router.push({ name: 'search', query: this.$route.query })\r\n      }\r\n    },\r\n    // 自定义事件的回调\r\n    trademarkInfo(trademark) {\r\n      // 1:整理品牌字段的参数 “ID:品牌名称”\r\n      this.searchParams.trademark = `${trademark.tmId}:${trademark.tmName}`;\r\n      // 再次发请求获取search模块列表数据进行展示\r\n      this.getData();\r\n    },\r\n    // 删除品牌的面包屑\r\n    removeTradeMark() {\r\n      // 将品牌信息清空\r\n      this.searchParams.trademark = undefined;\r\n      // 再次发请求\r\n      this.getData();\r\n    },\r\n    // 收集平台属性地方回调函数（自定义事件）\r\n    attrInfo(attr, attrValue) {\r\n      // [\"属性ID:属性值：属性名\"]\r\n      // 参数格式整理好\r\n      let props = `${attr.attrId}:${attrValue}:${attr.attrName}`;\r\n      // 数组去重\r\n      // if语句里面只要一条，可以省略大花括号\r\n      if (this.searchParams.props.indexOf(props) == -1) {\r\n        this.searchParams.props.push(props);\r\n      }\r\n      // 再次发送请求\r\n      this.getData();\r\n    },\r\n    // 删除售卖的属性\r\n    removeAttr(index){\r\n      // 再次整理参数\r\n      this.searchParams.props.splice(index,1)\r\n      // 再次发请求\r\n      this.getData();\r\n    },\r\n    // 排序的操作\r\n    changeOrder(flag){\r\n      // flage形参：标记，1是综合 2是价格\r\n      console.log(flag);\r\n      \r\n    }\r\n  },\r\n  // 数据监听：监听组件实例身上的属性的属性值变化\r\n  watch: {\r\n    // 监听路由的信息是否发生变化，如果发生变化，再次发起请求\r\n    $route(newValue, oldValue) {\r\n      // 再次发请求之前整理带给服务器的参数\r\n      Object.assign(this.searchParams, this.$route.query, this.$route.params)\r\n      // 再次发送请求\r\n      this.getData();\r\n      // 每一次请求，都一个吧相应的1,2,3级分类的id置空，让他接收下一次的\r\n      this.searchParams.category1Id = undefined;\r\n      this.searchParams.category2Id = undefined;\r\n      this.searchParams.category3Id = undefined;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.main {\r\n  margin: 10px 0;\r\n\r\n  .py-container {\r\n    width: 1200px;\r\n    margin: 0 auto;\r\n\r\n    .bread {\r\n      margin-bottom: 5px;\r\n      overflow: hidden;\r\n\r\n      .sui-breadcrumb {\r\n        padding: 3px 15px;\r\n        margin: 0;\r\n        font-weight: 400;\r\n        border-radius: 3px;\r\n        float: left;\r\n\r\n        li {\r\n          display: inline-block;\r\n          line-height: 18px;\r\n\r\n          a {\r\n            color: #666;\r\n            text-decoration: none;\r\n\r\n            &:hover {\r\n              color: #4cb9fc;\r\n    